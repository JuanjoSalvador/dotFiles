 de la derecha lista_puntos\n\t//¿que pasa cuando hago click en la lista?\n\t$('#lista_puntos li').on('click', function(e){\n\t\t$('#lista_puntos li').removeClass('seleccionado');\n\t\t$(this).addClass('seleccionado');\n\t\t$('#rosa').remove();\n\t\t$(this).append('<div id=\"rosa\"></div>');\n\t\tseleccion($(this).index());\n\t});\n\t//¿que pasa cuando hago click en los puntos?\n\t$('.puntos').on('click', function(e){\n\t\t$('#lista_puntos li').removeClass('seleccionado');\n\t\t$('#rosa').remove();\n\t\t$('#lista_puntos li').eq($(this).index()-1).append('<div id=\"rosa\"></div>');\n\t\t$('#lista_puntos li').eq($(this).index()-1).addClass('seleccionado');\n\t\tseleccion($(this).index()-1);\n\t});\n\treubicarMapa(600,0,0);\n}\n\n/**\n * Hace los movimientos de los objetos\n * @returns {undefined}\n */\nfunction reubicarMapa(zoom2, movX, movY) {\n\t$('#mapa').css({'background-size': zoom2 + 'px'});\n\t$('#objetos').css({'width': zoom2, 'height': (zoom2 * escalaAlto)});\n\t$('#mapa').css({'background-position': (-1) * movX + 'px ' + (-1) * movY + 'px'});\n\n\t$('#objetos').css({'top': (-1) * movY + 'px', 'left': (-1) * movX + 'px'});\n\t$('#mapa2').css({'top': movY + 'px', 'left': movX + 'px'});\n\n\n\n\n\t//Falta implementación escalado de las imagenes (ya está arreglado el zoom de arriba)\n\n\t//¡CUIDADO QUE NO VERIFICO EN LA SCRIPT LA POSICION DEL MAPA!\n\t//DEBE COMPROBARSE CUANDO SE INDIQUE LA POSICIÓN DE LOS PUNTOS MANUALMENTE.\n\t//OCULTAR Y MOSTRAR CAPA!\n\tvar puntos = $('.puntos');\n\tfor (i = 0; i < puntos.length; i++) {\n\t\tanchoObjetoT = parseFloat($('.puntos:eq(' + i + ')').css('width').split('px')[0]);\n\t\taltoObjetoT = parseFloat($('.puntos:eq(' + i + ')').css('height').split('px')[0]);\n\t\tdistanciaXObjeto = parseFloat($('.puntos:eq(' + i + ')').css('left').split('px')[0]);\n\t\tdistanciaYObjeto = parseFloat($('.puntos:eq(' + i + ')').css('top').split('px')[0]);\n\t\t\n\t\t\n\t\t//console.log('distancia al objetoX: ' + distanciaXObjeto + 'ancho del o